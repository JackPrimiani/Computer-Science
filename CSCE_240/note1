#include <iostream>
using std::cout;
using std::endl;
#include <fstream>
using std::fstream;
#include <string>
using std::string;

int main(int argc, char* argv){
  const //not finished line

  fstream fin;
  fin.open(kFName);
  
  if(!fin.is_open()) {
    cout  << kFName << " is not open." << endl;
    return 1;
  }

  int count = 0; // stores # of floating points
  fin count;


  double input = 0.0, total = 0.0;
  for(int i =0; i < count; i++){
    fin >> input;
    total += input;
  }

  cout << "average:" << (total/count) << endl;

  return 0
}
