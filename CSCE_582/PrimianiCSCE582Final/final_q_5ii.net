{
    position = (320 0);
    states = ("drill" "not-drill");
    HR_LinkGroup = "[Utility:0]";
    HR_LinkMode = "[Utility:0]";
}

node T
{
    position = (0 80);
    states = ("c" "o" "n");
}

node S
{
    position = (150 130);
    states = ("dr" "wt" "sk");
    HR_LinkGroup = "[T:0][Utility:0]";
    HR_LinkMode = "[T:0][Utility:0]";
}

